import turtle

aken = turtle.Screen()
aken.setup(width=500, height=400)
aken.title("Sinilill - Jaspar")
turtle.speed(0)
turtle.pensize(10)

#sinilill
turtle.pencolor("green")
turtle.goto(0, -150)
turtle.left(90)
turtle.fd(200)
turtle.left(-90)
turtle.begin_fill()
turtle.color("blue", "lightblue")
turtle.circle(70)
turtle.left(90)
turtle.fd(35)
turtle.end_fill()






turtle.done()
